{
  "format": "page",
  "id": "project_page",
  "label": "Project page",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "back_button_theme": {
        "type": "Select",
        "config": {
          "label": "Back button theme",
          "placeholder": "",
          "options": ["light", "dark"],
          "default_value": "light"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "archived": {
        "type": "Boolean",
        "config": {
          "label": "Archived",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": true
        }
      },
      "order_date": {
        "type": "Date",
        "config": {
          "label": "Order date",
          "placeholder": "Use for ordered projects on the home page and in the project footer."
        }
      },
      "creation_date": {
        "type": "Date",
        "config": {
          "label": "Creation date",
          "placeholder": ""
        }
      },
      "credits": {
        "type": "StructuredText",
        "config": {
          "label": "Credits",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink"
        }
      },
      "external_url": {
        "type": "Text",
        "config": {
          "label": "External url",
          "placeholder": ""
        }
      },
      "external_url_label": {
        "type": "Text",
        "config": {
          "label": "External url label",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "simple_content": {
              "type": "SharedSlice"
            },
            "media": {
              "type": "SharedSlice"
            },
            "cross_nav": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
